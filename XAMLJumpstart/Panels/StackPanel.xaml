<Page x:Class="XAMLJumpstart.Panels.StackPanel"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:XAMLJumpstart.Panels"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="StackPanel">

	<Grid>
		<StackPanel Orientation="Horizontal">
			<Rectangle Fill="Yellow" Height="50" Width="50"/>
			<Rectangle Fill="Green" Height="50" Width="50" />
			<Rectangle Fill="Blue" Height="50" Width="50" />
		</StackPanel>
	</Grid>
</Page>
